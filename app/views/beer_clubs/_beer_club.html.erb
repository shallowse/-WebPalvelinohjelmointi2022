<div id="<%= dom_id beer_club %>">
  <p>
    <strong>Name:</strong>
    <%= beer_club.name %>
  </p>

  <p>
    <strong>Founded:</strong>
    <%= beer_club.founded %>
  </p>

  <p>
    <strong>City:</strong>
    <%= beer_club.city %>
  </p>

  <p>
    <strong>members</strong>
  </p>

  <% beer_club.users.each do |user| %>
    <p><%= link_to user.username, user_path(user.id) %></p>
  <% end %>

</div>
